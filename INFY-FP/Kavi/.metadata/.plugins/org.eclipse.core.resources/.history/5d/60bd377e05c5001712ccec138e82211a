import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Solution {
	void show() {
		System.out.println("Show");
	}

	void getGreeting(String user) {
		System.out.println("Greeting" + user);
	}

	double calculateNetSalary(double basicSalary, float travelAllowance, float dearnessElements,
			float houseRentAllowance, float providentFund) {
		double netSalary = basicSalary + ((travelAllowance / 100) * basicSalary)
				+ ((dearnessElements / 100) * basicSalary) + ((houseRentAllowance / 100) * basicSalary)
				- ((providentFund / 100) * basicSalary);
		return netSalary;
	}

	public static void main(String[] args) throws IOException {
		BufferedReader bf = new BufferedReader(new InputStreamReader(System.in));
		String ch = bf.readLine();
		int i = Integer.valueOf(ch);
		System.out.println(ch + 3);
		System.out.println(i + 3);
		System.out.println("Hello World");

	}

}
