package org.task1;

import javax.persistence.Column;
import javax.persistence.Entity;

@Entity
public class Reservation {

	public Reservation() {
		// TODO Auto-generated constructor stub
	}

	public Reservation(String name, long phone, String fromPlace, String toPlace, int departDate, int arrivalDate) {
		super();
		this.name = name;
		this.phone = phone;
		this.fromPlace = fromPlace;
		this.toPlace = toPlace;
		this.departDate = departDate;
		this.arrivalDate = arrivalDate;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public long getPhone() {
		return phone;
	}

	public void setPhone(long phone) {
		this.phone = phone;
	}

	public String getFromPlace() {
		return fromPlace;
	}

	public void setFromPlace(String fromPlace) {
		this.fromPlace = fromPlace;
	}

	public String getToPlace() {
		return toPlace;
	}

	public void setToPlace(String toPlace) {
		this.toPlace = toPlace;
	}

	public int getDepartDate() {
		return departDate;
	}

	public void setDepartTime(int departDate) {
		this.departDate = departDate;
	}

	public int getArrivalDate() {
		return arrivalDate;
	}

	public void setArrivalTime(int arrivalDate) {
		this.arrivalDate = arrivalDate;
	}

	@Column(name = "C_NAME")
	private String name;
	@Column(name = "Ph_NO")
	private long phone;
	@Column(name = "F_PLACE")
	private String fromPlace;
	@Column(name = "T_PLACE")
	private String toPlace;
	private int departDate;
	private int arrivalDate;

}
